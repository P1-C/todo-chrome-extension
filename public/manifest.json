{
  "manifest_version": 3,
  "name": "Web-Todo",
  "version": "1.0",
  "description": "A Simple web todo extension to manage task",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}